<mvc:View xmlns:mvc="sap.ui.core.mvc"
		  xmlns:forms="sap.ui.layout.form"
		  xmlns="sap.m"
		  xmlns:core="sap.ui.core">

	<VBox>
		<forms:SimpleForm id="SimpleFormShipAddress"
			editable="false"
			layout="ResponsiveGridLayout"
			title="{i18n>Dir}"
			labelSpanL="3"
			labelSpanM="3"
			emptySpanL="4"
			emptySpanM="4"
			columnsL="2"
			columnsM="2" >
			<forms:content>
				<Label text="{i18n>detailName}" />
				<Text text="{objectView>/to_direccion/MC_NAME1}" />
				<Label text="{i18n>zipcity}" />
				<Text text="{objectView>/to_direccion/POST_CODE1} {objectView>/to_direccion/MC_CITY1}" />
				<Label text="{i18n>street}" />
				<Text text="{objectView>/to_direccion/STREET}" />	
				<Label text="{i18n>Number}" />
				<Text text="{objectView>/to_direccion/ADDRNUMBER}" />
				<Label text="{i18n>Region}" />
				<Text text="{objectView>/to_direccion/REGION}" />
				<Label text="{i18n>addGpo}" />
				<Text text="{objectView>/to_direccion/ADDR_GROUP}" />
			</forms:content>
		</forms:SimpleForm>
	</VBox>



</mvc:View>